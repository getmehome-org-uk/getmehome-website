{
    "name": "getmehome-website",
    "version": "1.0.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/getmehome-org-uk/getmehome-website.git"
    },
    "license": "MIT",
    "scripts": {
        "dev": "webpack-dev-server --open",
        "start": "webpack-dev-server --open",
        "build:clean": "rm -r ./dist || true",
        "build": "npm run build:clean && webpack --mode production && npm run copy:static",
        "copy:static": "cp ./static/* ./dist && npm run catch-about && npm run catch-register && npm run catch-privacy && npm run catch-links && npm run catch-contact",
        "catch-about": "cp ./dist/index.html ./dist/about.html",
        "catch-register": "cp ./dist/index.html ./dist/register.html",
        "catch-privacy": "cp ./dist/index.html ./dist/privacy.html",
        "catch-links": "cp ./dist/index.html ./dist/links.html",
        "catch-contact": "cp ./dist/index.html ./dist/contact.html",
        "deploy": "npm run deploy:dist && npm run deploy:alias",
        "deploy:dist": "surge ./dist --domain https://getmehome.org.uk",
        "deploy:alias": "npm run deploy:alias1 && npm run deploy:alias2",
        "deploy:alias1": "surge ./alias1 --domain https://getmehome.uk",
        "deploy:alias2": "surge ./alias2 --domain https://getmehome.org",
        "go": "npm run build && npm run deploy"
    },
    "dependencies": {
        "history": "^4.10.1",
        "react": "^16.13.0",
        "react-bootstrap": "^1.0.0-beta.17",
        "react-dom": "^16.13.0",
        "react-jsonschema-form": "^1.8.1",
        "react-redux": "^7.2.0",
        "react-router-bootstrap": "^0.25.0",
        "react-router-dom": "^5.1.2",
        "react-select": "^3.0.8",
        "redux": "^4.0.5",
        "redux-logger": "^4.0.0",
        "redux-thunk": "^2.3.0",
        "swr": "^0.1.18"
    },
    "devDependencies": {
        "@babel/core": "^7.4.3",
        "@babel/preset-env": "^7.4.3",
        "@babel/preset-react": "^7.0.0",
        "babel-loader": "^8.0.5",
        "css-loader": "^3.4.2",
        "html-webpack-plugin": "^3.2.0",
        "node-sass": "^4.13.1",
        "path": "^0.12.7",
        "sass-loader": "^8.0.2",
        "style-loader": "^1.1.3",
        "webpack": "^4.42.0",
        "webpack-cli": "^3.3.0",
        "webpack-dev-server": "^3.2.1"
    }
}
